{
  "name": "frontend-starter-kit",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/springload/frontend-starter-kit"
  },
  "engines": {
    "node": "^6.8.0",
    "npm": "^3.10.8"
  },
  "scripts": {
    "start": "npm run lint:versions -s && npm run watch -s",
    "lint:versions": "NVM_VERSION=$(cat .nvmrc); NODE_VERSION=$(node --version); echo $NODE_VERSION | grep -q $NVM_VERSION && echo \"✓ running node $NODE_VERSION - all clear\" || (echo \"✗ running node $NODE_VERSION - expected $NVM_VERSION. Use nvm to install the right version.\" && exit 1)",
    "linter:js": "eslint",
    "linter:js:ci": "eslint --no-eslintrc --config .ci-eslintrc",
    "linter:sass": "sass-lint -v -q",
    "lint:js": "npm run linter:js -s -- .",
    "lint:js:ci": "npm run linter:js:ci -s -- .",
    "lint:sass": "npm run linter:sass -s",
    "report:css": "gulp report:css",
    "lint": "npm run lint:versions -s && npm run lint:js -s && npm run lint:sass -s",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "./bin/ci-test.sh",
    "build": "NODE_ENV=development gulp build",
    "dist": "NODE_ENV=production gulp build",
    "css": "gulp css",
    "critical:css": "gulp critical:css",
    "js": "gulp js",
    "svg": "gulp svg",
    "watch": "gulp watch",
    "docs": "mkdir -p core/static && npm run build -s && NODE_ENV=development node --harmony ./docs/index.js",
    "lock": "npm run lint:versions -s && npm shrinkwrap --dev && sed -i '' 's/https:/http:/g' npm-shrinkwrap.json",
    "deploy": "git push origin master:deploy/production"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-eslint": "^6.0.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-sync": "^2.8.0",
    "browserify": "^13.0.0",
    "browserify-incremental": "^3.0.0",
    "bundle-collapser": "^1.2.1",
    "critical": "^0.7.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "express": "^4.13.0",
    "gulp": "^3.9.0",
    "gulp-inject": "^3.0.0",
    "gulp-parker": "^0.1.0",
    "gulp-pleeease": "^2.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.1.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-svgmin": "^1.1.2",
    "gulp-svgstore": "^5.0.1",
    "gulp-uglify": "^1.4.0",
    "gulp-util": "^3.0.4",
    "hyperlink": "^2.3.0",
    "iconv": "^2.1.11",
    "jest": "^16.0.2",
    "loose-envify": "^1.2.0",
    "react-test-renderer": "^15.3.2",
    "sass-lint": "1.5.0",
    "sass-module-importer": "^1.2.1",
    "uglifyify": "^3.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "flickity": "^1.1.2",
    "focus-trap": "^1.0.1",
    "h5bp-print-styles": "springload/h5bp-print-styles#master",
    "normalize.css": "^4.1.1",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "springload-analytics.js": "^2.0.0",
    "tabbable": "^1.0.0",
    "youtube-player": "^3.0.2"
  }
}
